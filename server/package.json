{
 "name": "server",
 "version": "1.0.0",
 "author": "Diego Chiquero Mena",
 "description": "Nuxt-crud server",
 "license": "MIT",
 "homepage": "https://github.com/diegochiquero/nuxt-crud/server/#readme",
 "main": "app.js",
 "scripts": {
  "start": "node app.js",
  "start:prod": "env NODE_ENV=production node app.js",
  "start:dev": "env NODE_ENV=development nodemon server/app.js",
  "start:bdd": "env NODE_ENV=test nodemon server/app.js",
  "test": "env NODE_ENV=test mocha --recursive --timeout 60000 --exit"
 },
 "keywords": [
  "MongoDB",
  "Express",
  "Node",
  "Docker",
  "Mocha"
 ],
 "dependencies": {
  "body-parser": "^1.19.0",
  "config": "^3.3.1",
  "cors": "^2.8.5",
  "dotenv": "^8.2.0",
  "errorhandler": "^1.5.1",
  "express": "^4.17.1",
  "helmet": "^3.23.3",
  "method-override": "^3.0.0",
  "mongoose": "^5.9.5",
  "mongoose-unique-validator": "^2.0.3",
  "morgan": "^1.10.0"
 },
 "devDependencies": {
  "chai": "^4.2.0",
  "mocha": "^8.1.3",
  "mongodb-memory-server": "^6.6.1",
  "nodemon": "^2.0.4",
  "supertest": "^4.0.2"
 }
}
